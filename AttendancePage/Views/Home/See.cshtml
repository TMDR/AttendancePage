
<style>
        table {
            font-family: "Courier New", Courier, monospace;
            border: 4px solid #555555;
            background-color: #555454;
            width: 400px;
            text-align: center;
            border-collapse: collapse;
        }

            table td, table th {
                border: 1px solid #555555;
                padding: 5px 10px;
            }

            table tbody td {
                font-size: 12px;
                font-weight: bold;
                color: #FFFFFF;
            }

            table td:nth-child(even) {
                background: #398AA4;
            }

            table thead {
                background: #398AA4;
                border-bottom: 10px solid #398AA4;
            }

                table thead th {
                    font-size: 15px;
                    font-weight: bold;
                    color: #FFFFFF;
                    text-align: left;
                    border-left: 2px solid #398AA4;
                }

                    table thead th:first-child {
                        border-left: none;
                    }

            table tfoot td {
                font-size: 13px;
            }

            table tfoot .links {
                text-align: right;
            }

                table tfoot .links a {
                    display: inline-block;
                    background: #FFFFFF;
                    color: #398AA4;
                    padding: 2px 8px;
                    border-radius: 5px;
                }


        body ,dialog{
            font-family: 'Open Sans', sans-serif;
            margin: 0 auto 0 auto;
            width: 100%;
            text-align: center;
            margin: 20px 0px 20px 0px;
            background-color:transparent;
        }

        p {
            font-size: 12px;
            text-decoration: none;
            color: #ffffff;
        }

        h1 {
            font-size: 1.5em;
            color: #525252;
        }

        .box {
            background: white;
            width: 300px;
            border-radius: 6px;
            margin: 0 auto 0 auto;
            padding: 0px 0px 70px 0px;
            border: #2980b9 4px solid;
        }

        .email {
            background: #ecf0f1;
            border: #ccc 1px solid;
            border-bottom: #ccc 2px solid;
            padding: 8px;
            width: 250px;
            color: #AAAAAA;
            margin-top: 10px;
            font-size: 1em;
            border-radius: 4px;
        }
         .passsword {
            background: #ecf0f1;
            border: #ccc 1px solid;
            border-bottom: #ccc 2px solid;
            padding: 8px;
            width: 250px;
            color: #AAAAAA;
            margin-top: 10px;
            font-size: 1em;
            border-radius: 4px;
        }


        .password {
            border-radius: 4px;
            background: #ecf0f1;
            border: #ccc 1px solid;
            padding: 8px;
            width: 250px;
            font-size: 1em;
        }

        .btn {
            background: #2ecc71;
            width: 125px;
            padding-top: 5px;
            padding-bottom: 5px;
            color: white;
            border-radius: 4px;
            border: #27ae60 1px solid;
            margin-top: 20px;
            margin-bottom: 20px;
            float: left;
            margin-left: 16px;
            font-weight: 800;
            font-size: 0.8em;
        }

            .btn:hover {
                background: #2CC06B;
            }

        #btn2 {
            float: left;
            background: #3498db;
            width: 125px;
            padding-top: 5px;
            padding-bottom: 5px;
            color:  white;
            border-radius: 4px;
            border: #2980b9 1px solid;
            margin-top: 20px;
            margin-bottom: 20px;
            margin-left: 10px;
            font-weight: 800;
            font-size: 0.8em;
        }

            #btn2:hover {
                background: #3594D2;
            }
            #dialog{
                position:absolute ;
                z-index:1;
                top:0px;
                left:65%;
                border:none;
            }
            #frameC{
                position:absolute ;
                z-index:1;
                top:25%;
                left:55%;
                border:none;
            }
            iframe{
                height:500px;
                top:0px;
                border:none;
            }

                                ::-webkit-scrollbar {
                width: 20px;
                }

::-webkit-scrollbar-track {
  box-shadow: inset 0 0 5px grey;
  border-radius: 10px;
}

::-webkit-scrollbar-thumb {
  background-color: transparent;
  border-radius: 10px;
            }
::-webkit-scrollbar-thumb:hover{
    background-color:#555454;
}
::-webkit-scrollbar-corner{
    background-color:transparent;
}
</style>
<html>
<head>
</head>
<body>
    <div class="container-login100" style="top:15% ; position:absolute">
        <table>
            <thead>
                <tr><th>UserName</th><th>Total Duration</th><th>Rate Per Hour</th><th>Salalry</th></tr>
            </thead>
            <tbody>
                @foreach (AttendancePage.Models.Worker worker in new dbcontext().Worker)
                {@if (!worker.Email.Equals("admin0")) {
                    <tr><td>@Html.Name(worker.Email)</td><td>@Html.Name(AttendancePage.Controllers.HomeController.Duration[worker.Email].ToString())</td>
                <td>@Html.Name(worker.RatePerHour.ToString()) SR</td><td>@Html.Name((worker.RatePerHour* AttendancePage.Controllers.HomeController.Duration[worker.Email]).ToString()) SR</td></tr>
                    }
                }
            </tbody>
        </table>
        @foreach (int day in AttendancePage.Controllers.HomeController.seeDay)
        {<table>
                <thead>
                    <tr>
                        <th>Day : @Html.Name(day.ToString())</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                    <tr>
                        <td>UserName&nbsp;&nbsp;&nbsp;</td>
                        <td>&nbsp;&nbsp;Date&nbsp;&nbsp;</td>
                        <td>&nbsp;&nbsp;TimeIn&nbsp;</td>
                        <td>&nbsp;&nbsp;TimeOut&nbsp;</td>
                        <td>&nbsp;&nbsp;Duration</td>
                    </tr>
                    @foreach (AttendancePage.Models.Worker worker in new dbcontext().Worker)
                    {
                        @foreach (AttendancePage.Models.DateTime11 date in AttendancePage.Controllers.HomeController.seeTime.Where(e => e.Date.Value.Day == day && e.Email == worker.Email).OrderBy(e => e.TimeIn))
                        {
                            <tr>
                                <td>@Html.Name(date.Email)</td>
                                <td>&nbsp;&nbsp; @Html.Name(date.Date.Value.Day.ToString())/@Html.Name(date.Date.Value.Month.ToString())/@Html.Name(date.Date.Value.Year.ToString())&nbsp;&nbsp;</td>
                                <td>&nbsp;&nbsp; @Html.Name(date.TimeIn.ToString()) &nbsp;&nbsp;</td>
                                @if (date.TimeOut.HasValue)
                                {
                                    <td>&nbsp;&nbsp; @Html.Name(date.TimeOut.ToString()) &nbsp;&nbsp;</td>
                                }
                                @if (date.TimeIn.HasValue && date.TimeOut.HasValue)
                                {
                                    <td>
                                        &nbsp;&nbsp;
                                        @if (date.TimeOut.Value.Subtract(date.TimeIn.Value).Minutes > 30)
                                        {
                                            @Html.Name((date.TimeOut.Value.Subtract(date.TimeIn.Value).Hours + 1).ToString())
                                        }
                                        @if (date.TimeOut.Value.Subtract(date.TimeIn.Value).Minutes <= 30)
                                        {
                                            @Html.Name((date.TimeOut.Value.Subtract(date.TimeIn.Value).Hours).ToString())
                                        }H&nbsp;&nbsp;
                                    </td>
                                }
                            </tr>
                        }
                    }
                </tbody>
            </table>
        }
    </div>
</body>
</html>